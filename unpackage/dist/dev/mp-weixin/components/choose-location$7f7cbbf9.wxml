<import src="page-head$7d3e75a2" />
<template name="choose-location$7f7cbbf9">
  <view class="_view data-v-912528f8">
    <template data="{{...$root[$kk+'0'], $root}}" is="page-head$7d3e75a2"></template>
    <view class="_view data-v-912528f8 page-body">
      <view class="_view data-v-912528f8 page-section">
        <view class="_view data-v-912528f8 page-body-info">
          <text class="_text data-v-912528f8 page-body-text-small">当前位置信息</text>

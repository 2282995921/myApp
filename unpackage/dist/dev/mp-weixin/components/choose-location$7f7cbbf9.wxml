            </view>
          </block>
        </view>
        <view class="_view data-v-912528f8 btn-area">
          <button type="primary" bindtap="handleProxy" data-eventid="{{'0'}}" data-comkey="{{$k}}" class="_button data-v-912528f8">选择位置</button>
          <button bindtap="handleProxy" data-eventid="{{'1'}}" data-comkey="{{$k}}" class="_button data-v-912528f8">清空</button>
        </view>
      </view>
    </view>
  </view>
</template>